{
  "theme": {
    "typography": {
      "config": {
        "currentFont": {
          "$type": "string",
          "$value": "public-sans",
          "$description": "Current font family selection"
        },
        "normalizationFactor": {
          "$type": "number",
          "$value": "{core.typography.normalization.factors.public-sans}",
          "$description": "Current font's normalization factor"
        }
      },
      "fontSize": {
        "50": {
          "$type": "fontSizes",
          "$value": "{core.typography.fontSize.50} * {theme.typography.config.normalizationFactor}",
          "$description": "Font size 50 - automatically normalized based on current font"
        },
        "100": {
          "$type": "fontSizes",
          "$value": "{core.typography.fontSize.100} * {theme.typography.config.normalizationFactor}"
        },
        "200": {
          "$type": "fontSizes",
          "$value": "{core.typography.fontSize.200} * {theme.typography.config.normalizationFactor}"
        },
        "300": {
          "$type": "fontSizes",
          "$value": "{core.typography.fontSize.300} * {theme.typography.config.normalizationFactor}"
        },
        "400": {
          "$type": "fontSizes",
          "$value": "{core.typography.fontSize.400} * {theme.typography.config.normalizationFactor}"
        },
        "500": {
          "$type": "fontSizes",
          "$value": "{core.typography.fontSize.500} * {theme.typography.config.normalizationFactor}"
        },
        "600": {
          "$type": "fontSizes",
          "$value": "{core.typography.fontSize.600} * {theme.typography.config.normalizationFactor}"
        },
        "700": {
          "$type": "fontSizes",
          "$value": "{core.typography.fontSize.700} * {theme.typography.config.normalizationFactor}"
        },
        "800": {
          "$type": "fontSizes",
          "$value": "{core.typography.fontSize.800} * {theme.typography.config.normalizationFactor}"
        },
        "900": {
          "$type": "fontSizes",
          "$value": "{core.typography.fontSize.900} * {theme.typography.config.normalizationFactor}"
        }
      }
    }
  }
}
